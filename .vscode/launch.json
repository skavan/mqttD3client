{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch index.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceFolder}/index.html"
        },
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "file": "${workspaceFolder}/index.html"
        },
        {
            "name": "Launch newtest",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "file": "${workspaceFolder}/newtest.html"
        },
        {
            "name": "Launch pageResize",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "file": "${workspaceFolder}/pageResize.html"
        },
        {
            "name": "Launch semantic",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "file": "${workspaceFolder}/semantic.html"
        }
    ]
}